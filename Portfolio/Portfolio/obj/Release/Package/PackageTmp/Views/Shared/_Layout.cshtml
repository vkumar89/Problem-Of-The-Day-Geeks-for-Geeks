<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/Site.css">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript">
        function checkAccess() {
            var username = document.getElementById("username").value;
            if (username.toLowerCase() === "admin") {
                document.getElementById("adminLink").style.display = "inline";
                document.getElementById("message").style.display = "none";
            } else {
                document.getElementById("adminLink").style.display = "none";
                alert("User is not authorized");
            }
        }
    </script>
</head>
<body>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-dark">
        <div class="container">
            @*@Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
            <button type="button" class="navbar-toggler" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" title="Toggle navigation" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    <li>@Html.ActionLink("Home", "DisplayProjects", "Projects", new { area = "" }, new { @class = "nav-link" })</li>
                    <li><a class="nav-link" href="#about">AboutMe</a></li>
                    <li><a class="nav-link" href="#portfolio">Skills</a></li>
                    <li><a href="~/Uploads/vipin resume 1.5.pdf" download class="nav-link text-decoration-none ">Resume</a></li>
                    <li><a class="nav-link" href="#Projects">Projects</a></li>
                    <li>

                    </li>
                    <div class="input-group mb-3 align-content-end collapse navbar-collapse">
                        <input type="text" id="username" placeholder="Username" class="form-control">
                        <button class="btn btn-secondary" onclick="checkAccess()">Admin Access</button>

                        <div id="adminLink" style="display:none;">

                            <button class="btn btn-info nav-link">@Html.ActionLink("Admin", "Admin", "Projects", new { @class = "text-decoration-none text-white " })</button>
                        </div>
                        <div id="message" style="display:none;">
                            <p>User is not authorized</p>
                        </div>
                    </div>
                </ul>

                
            </div>
            
</nav>

